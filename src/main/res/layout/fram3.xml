<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/rl_003"
    android:focusable="true"
    android:focusableInTouchMode="true">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="气洗："
        android:id="@+id/textView2"
        android:textSize="22dp"
        android:layout_below="@+id/textView46"
        android:layout_alignRight="@+id/textView46"
        android:layout_marginTop="20dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="气液混合洗："
        android:id="@+id/textView44"
        android:textSize="22dp"
        android:layout_toStartOf="@+id/textView2"
        android:layout_below="@+id/textView2"
        android:layout_alignRight="@+id/textView46"
        android:layout_marginTop="20dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="反洗："
        android:id="@+id/textView45"
        android:textSize="22dp"
        android:layout_below="@+id/textView44"
        android:layout_toStartOf="@+id/textView47"
        android:layout_marginTop="20dp"
        android:layout_alignRight="@+id/textView46" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="清洗周期："
        android:id="@+id/textView46"
        android:textSize="22dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="150dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="清洗时间设定"
        android:id="@+id/textView47"
        android:textColor="#18c9a2"
        android:layout_alignParentTop="true"
        android:layout_marginTop="30dp"
        android:layout_centerHorizontal="true" />

    <EditText
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/et_fx"
        android:text="10"
        android:textSize="22dp"
        android:paddingBottom="3dp"
        android:layout_toEndOf="@+id/textView45"
        android:layout_below="@+id/et_qy"
        android:layout_marginTop="15dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="秒"
        android:id="@+id/textView48"
        android:textSize="22dp"
        android:layout_alignTop="@+id/textView2"
        android:layout_toEndOf="@+id/et_cycle" />

    <EditText
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/et_qy"
        android:text="10"
        android:textSize="22dp"
        android:paddingBottom="3dp"
        android:layout_toEndOf="@+id/textView44"
        android:layout_below="@+id/et_qx"
        android:layout_marginTop="15dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="秒"
        android:id="@+id/textView49"
        android:textSize="22dp"
        android:layout_toEndOf="@+id/et_cycle"
        android:layout_alignTop="@+id/textView46" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="秒"
        android:id="@+id/textView50"
        android:textSize="22dp"
        android:layout_toEndOf="@+id/et_cycle"
        android:layout_alignTop="@+id/textView44" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="秒"
        android:id="@+id/textView51"
        android:textSize="22dp"
        android:layout_alignTop="@+id/textView45"
        android:layout_toEndOf="@+id/et_cycle" />

    <EditText
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/et_cycle"
        android:text="20"
        android:textSize="22dp"
        android:layout_toEndOf="@+id/textView46"
        android:layout_marginTop="145dp"
        android:paddingBottom="3dp" />

    <EditText
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/et_qx"
        android:text="10"
        android:textSize="22dp"
        android:paddingBottom="3dp"
        android:layout_toEndOf="@+id/textView2"
        android:layout_below="@+id/et_cycle"
        android:layout_marginTop="15dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="默认"
        android:id="@+id/bt_mr"
        android:layout_marginTop="83dp"
        android:textSize="20dp"
        android:background="#5aaca8"
        android:layout_below="@+id/et_fx"
        android:layout_toStartOf="@+id/textView45" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="确定"
        android:id="@+id/bt_qd"
        android:layout_alignBottom="@+id/bt_mr"
        android:layout_toEndOf="@+id/et_fx"
        android:textSize="20dp"
        android:background="#5aaca8" />
</RelativeLayout>